# PostgreSQL Database Configuration
POSTGRES_DB=suburbcompass
POSTGRES_USER=user
POSTGRES_PASSWORD=suburbcompass

# API Service Configuration
# The DATABASE_URL should use the same credentials as above.
DATABASE_URL=postgresql://user:suburbcompass@db:5432/suburbcompass
# Use a long, random, and secret string for JWT_SECRET
JWT_SECRET=your_super_secret_jwt_string_here
# Get this from your Stripe dashboard
STRIPE_SECRET_KEY=your_stripe_sk_live_or_test_key_here

# AI Service Configuration
# Get this from your OpenAI dashboard
OPENAI_API_KEY=your_openai_api_key_here